<mujoco model="freight">
    <option gravity="0 0 -9.81" />
    <compiler angle="radian" inertiafromgeom="true" meshdir="../meshes/"/>

    <statistic meansize="0.0963259" extent="0.878747" center="0.001363 1.45929e-05 0.186545"/>
    <option integrator="RK4" timestep="0.01"/>
    <default>
        <joint armature="0" damping="0" limited="false"/>

        <geom conaffinity="0" condim="3" friction="1 0.5 0.5" rgba="0.2 0.2 0.2 1"/>
    </default>
    
    <asset>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <mesh name="base_link" file="base_link.STL"/>
        <mesh name="base_link_collision" file="base_link_collision.STL"/>
        <mesh name="r_wheel_link" file="r_wheel_link.STL"/>
        <mesh name="r_wheel_link_collision" file="r_wheel_link_collision.STL"/>
        <mesh name="l_wheel_link" file="l_wheel_link.STL"/>
        <mesh name="l_wheel_link_collision" file="l_wheel_link_collision.STL"/>
        <mesh name="estop_link" file="estop_link.STL"/>
        <mesh name="estop_link_collision" file="estop_link_collision.STL"/>
        <mesh name="laser_link" file="laser_link.STL"/>
        <mesh name="laser_link_collision" file="laser_link_collision.STL"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom friction="1.0 .1 .1" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="20 20 0.125" type="plane" condim="3" conaffinity="1"/>
        
        <body name="chassis" pos="0 0 0" euler='0 0 0'>
            <joint type="free"/>

            <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.356 0.361 0.376 1" mesh="base_link"/>
            <geom type="mesh" contype="1" conaffinity="1" rgba="0.356 0.361 0.376 1" friction="0.3 0.1 0.1" mesh="base_link_collision" priority="1"/>

            <geom pos="-0.12465 0.23892 0.31127" quat="0.707105 0.707108 0 0" type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.8 0 0 1" mesh="estop_link"/>
            <geom pos="-0.12465 0.23892 0.31127" quat="0.707105 0.707108 0 0" type="mesh" rgba="0.8 0 0 1" mesh="estop_link_collision"/>

            <geom pos="0.235 0 0.2878" quat="0 1 0 0" type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.792157 0.819608 0.933333 1" mesh="laser_link"/>
            <geom pos="0.235 0 0.2878" quat="0 1 0 0" type="mesh" rgba="0.792157 0.819608 0.933333 1" mesh="laser_link_collision"/>

            <body name="r_wheel_link" pos="0.0012914 -0.18738 0.055325" euler='1.5708 0 0'>
                <inertial pos="0 0 0" mass="4.3542" diaginertia="0.005 0.0045 0.0045"/>
                <joint name="r_wheel_joint" pos="0 0 0" axis="0 0 -1" type="hinge" damping="0.1"/>
                <geom size="0.058 0.058 0.05" type="ellipsoid" contype="1" conaffinity="1" />
            </body>
            <body name="l_wheel_link" pos="0.0012914 0.18738 0.055325" euler='1.5708 0 0'>
                <inertial pos="0 0 0" mass="4.3542" diaginertia="0.005 0.0045 0.0045"/>
                <joint name="l_wheel_joint" pos="0 0 0" axis="0 0 -1" type="hinge" damping="0.1"/>
                <geom size="0.058 0.058 0.05" type="ellipsoid" contype="1" conaffinity="1" />
            </body>
        </body>
    </worldbody>

    <actuator>
        <motor joint="r_wheel_joint" ctrlrange="-1 1" ctrllimited="true"/>
        <motor joint="l_wheel_joint" ctrlrange="-1 1" ctrllimited="true"/>
    </actuator>
</mujoco>
